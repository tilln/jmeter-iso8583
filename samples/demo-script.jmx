          <JSR223PreProcessor guiclass="TestBeanGUI" testclass="JSR223PreProcessor" testname="Increment ATC" enabled="true">
            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="script">int atc = props.atcs[vars.PAN] ?: Long.valueOf(vars.ATC ?: &apos;0&apos;, 16).intValue()
props.atcs[vars.PAN] = ++atc
vars.put(&apos;ATC&apos;, sprintf(&apos;%04x&apos;, atc))
</stringProp>
            <stringProp name="scriptLanguage">groovy</stringProp>
          </JSR223PreProcessor>

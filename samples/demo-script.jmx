            <stringProp name="filename"></stringProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="script">sampler.addField(&apos;55.16&apos;, vars.PAN, &apos;5A&apos;)
sampler.addField(&apos;55.17&apos;, vars.PSN, &apos;5F34&apos;)

sampler.removeField(&apos;52&apos;)

def emv_pan = sampler.getFields().find { f -&gt; f.getName().startsWith(&apos;55.&apos;) &amp;&amp; f.getTag() == &apos;5A&apos; }
log.info &quot;${emv_pan}&quot;

def isomsg = sampler.getRequest()

log.info &quot;5A=&gt;${isomsg.getString(emv_pan.getName())}&quot;

isomsg.setHeader(&apos;DemoJMeter&apos;.bytes) // must be the same length as per Config element
</stringProp>
